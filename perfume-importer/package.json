{
  "name": "perfume-importer",
  "version": "1.0.0",
  "content": "향수 데이터 가져오기 배치 프로그램",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "import": "ts-node src/batch/importPerfume.ts",
    "update-images": "ts-node src/batch/updatePerfumeImages.ts"
  },
  "keywords": [
    "perfume",
    "batch",
    "import",
    "mikroorm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mikro-orm/cli": "^5.7.12",
    "@mikro-orm/core": "^5.7.12",
    "@mikro-orm/migrations": "^5.7.12",
    "@mikro-orm/mysql": "^5.7.12",
    "@mikro-orm/reflection": "^5.7.12",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.34.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/config/mikro-orm.config.ts",
      "./dist/config/mikro-orm.config.js"
    ]
  }
}
