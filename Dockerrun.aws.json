{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "${{ msjungdev }}/${{ scentist }}:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": 8080,
      "HostPort": 80
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/elasticsearch-data",
      "ContainerDirectory": "/usr/share/elasticsearch/data"
    }
  ],
  "Logging": "/var/log/app"
}
